@model IEnumerable<dynamic>
<table>
    <thead>
        <tr>
            <th>Subject Name</th>
            <th>Count of Students with > 90 Marks</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var subject in Model)
        {
            <tr>
                <td>@subject.SubjectName</td>
                <td>@subject.Count</td>
            </tr>
        }
    </tbody>
</table>
